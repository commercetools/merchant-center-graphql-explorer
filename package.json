{
  "name": "merchant-center-graphql-explorer",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "prebuild": "yarn compile-data",
    "build": "mc-scripts build",
    "start": "dotenv -- mc-scripts start",
    "compile-html": "NODE_ENV=production dotenv -- mc-scripts compile-html --transformer $(pwd)/vercel-deployment/transformer.js",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development dotenv -- mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=$(pwd)/intl-formatter.js --out-file=$(pwd)/src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "compile-data": "formatjs compile-folder --format=$(pwd)/intl-formatter.js --ast src/i18n/data src/i18n/compiled-data",
    "lint": "jest --config jest.eslint.config.js",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:e2e": "cypress run",
    "versions:appkit": "bulk-update-versions --match '@commercetools-frontend/(.*)'",
    "versions:uikit": "bulk-update-versions --match '@commercetools-uikit/(.*)'"
  },
  "dependencies": {
    "@apollo/client": "3.3.7",
    "@commercetools-frontend/actions-global": "18.1.0",
    "@commercetools-frontend/application-components": "18.1.0",
    "@commercetools-frontend/application-shell": "18.1.3",
    "@commercetools-frontend/assets": "18.1.0",
    "@commercetools-frontend/i18n": "18.1.0",
    "@commercetools-frontend/permissions": "18.1.0",
    "@commercetools-uikit/flat-button": "10.44.4",
    "@commercetools-uikit/icons": "10.44.4",
    "@commercetools-uikit/notifications": "10.44.4",
    "@commercetools-uikit/spacings": "10.44.4",
    "@commercetools-uikit/text": "10.44.4",
    "graphiql": "1.3.2",
    "graphiql-explorer": "0.6.2",
    "graphql": "15.4.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "5.10.18",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@commercetools-frontend/eslint-config-mc-app": "18.0.0",
    "@commercetools-frontend/jest-preset-mc-app": "18.1.0",
    "@commercetools-frontend/mc-scripts": "18.1.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@formatjs/cli": "3.0.5",
    "@testing-library/cypress": "7.0.3",
    "@testing-library/react": "11.2.3",
    "bulk-update-versions": "1.1.2",
    "cypress": "6.3.0",
    "dotenv": "8.2.0",
    "dotenv-cli": "4.0.0",
    "eslint": "7.18.0",
    "eslint-formatter-pretty": "4.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-runner-eslint": "0.10.0",
    "jest-silent-reporter": "0.4.0",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.3",
    "start-server-and-test": "1.11.7",
    "vercel": "21.2.0"
  },
  "resolutions": {
    "**/cypress": "6.3.0"
  }
}
