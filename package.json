{
  "name": "merchant-center-graphql-explorer",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "yarn compile-data && mc-scripts build --build-only",
    "start": "mc-scripts start",
    "compile-html": "NODE_ENV=production mc-scripts compile-html",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=./intl-formatter.js --out-file=./src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "compile-data": "formatjs compile-folder --format=./intl-formatter.js --ast src/i18n/data src/i18n/compiled-data",
    "lint": "jest --config jest.eslint.config.js",
    "format": "prettier --write '**/*.js'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:e2e": "cypress run",
    "versions:appkit": "yarn up -E '@commercetools-frontend/*'",
    "versions:uikit": "yarn up -E '@commercetools-uikit/*'",
    "deploy": "yarn compile-html && cp public/index.html public/200.html && surge public $APPLICATION_URL"
  },
  "dependencies": {
    "@apollo/client": "3.5.7",
    "@commercetools-frontend/actions-global": "21.0.0",
    "@commercetools-frontend/application-components": "21.2.0",
    "@commercetools-frontend/application-shell": "21.2.0",
    "@commercetools-frontend/assets": "21.0.0",
    "@commercetools-frontend/cypress": "21.2.0",
    "@commercetools-frontend/eslint-config-mc-app": "21.0.0",
    "@commercetools-frontend/i18n": "21.2.0",
    "@commercetools-frontend/jest-preset-mc-app": "21.0.0",
    "@commercetools-frontend/mc-scripts": "21.1.2",
    "@commercetools-frontend/permissions": "21.0.0",
    "@commercetools-frontend/sdk": "21.1.0",
    "@commercetools-uikit/flat-button": "14.0.1",
    "@commercetools-uikit/icons": "14.0.1",
    "@commercetools-uikit/notifications": "14.0.1",
    "@commercetools-uikit/spacings": "14.0.1",
    "@commercetools-uikit/text": "14.0.1",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@formatjs/cli": "4.8.2",
    "@manypkg/cli": "0.19.1",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/react": "12.1.3",
    "cypress": "9.5.1",
    "dotenv": "16.0.0",
    "eslint": "8.10.0",
    "eslint-formatter-pretty": "4.1.0",
    "graphiql": "1.7.0",
    "graphiql-explorer": "0.9.0",
    "graphql": "15.8.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-runner-eslint": "1.0.0",
    "jest-silent-reporter": "0.5.0",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.3.5",
    "prettier": "2.5.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.24.6",
    "react-redux": "7.2.6",
    "react-router-dom": "6.3.0",
    "redux": "4.1.2",
    "start-server-and-test": "1.14.0",
    "surge": "0.23.1"
  },
  "packageManager": "yarn@3.1.1",
  "license": "MIT"
}
