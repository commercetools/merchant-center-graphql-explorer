{
  "name": "merchant-center-graphql-explorer",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "dotenv -- mc-scripts start",
    "compile-html": "NODE_ENV=production dotenv -- mc-scripts compile-html --transformer $(pwd)/vercel-deployment/transformer.js",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development dotenv -- mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=$(pwd)/intl-formatter.js --out-file=$(pwd)/src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "compile-data": "formatjs compile-folder --format=$(pwd)/intl-formatter.js --ast src/i18n/data src/i18n/compiled-data",
    "lint": "jest --config jest.eslint.config.js",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:e2e": "cypress run",
    "versions:appkit": "bulk-update-versions --match '@commercetools-frontend/(.*)'",
    "versions:uikit": "bulk-update-versions --match '@commercetools-uikit/(.*)'"
  },
  "dependencies": {
    "@apollo/client": "3.2.5",
    "@commercetools-frontend/actions-global": "17.1.1",
    "@commercetools-frontend/application-components": "17.1.1",
    "@commercetools-frontend/application-shell": "17.1.1",
    "@commercetools-frontend/assets": "16.15.3",
    "@commercetools-frontend/i18n": "17.1.1",
    "@commercetools-frontend/mc-http-server": "16.18.0",
    "@commercetools-frontend/permissions": "17.1.1",
    "@commercetools-uikit/flat-button": "10.36.0",
    "@commercetools-uikit/icons": "10.38.0",
    "@commercetools-uikit/notifications": "10.38.0",
    "@commercetools-uikit/spacings": "10.18.7",
    "@commercetools-uikit/text": "10.36.0",
    "graphiql": "1.0.6",
    "graphiql-explorer": "0.6.2",
    "graphql": "15.4.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "5.8.8",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@commercetools-frontend/eslint-config-mc-app": "17.1.0",
    "@commercetools-frontend/jest-preset-mc-app": "17.1.0",
    "@commercetools-frontend/mc-scripts": "17.1.2",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@formatjs/cli": "2.13.2",
    "@testing-library/cypress": "7.0.1",
    "@testing-library/react": "11.1.0",
    "babel-eslint": "10.1.0",
    "bulk-update-versions": "1.1.0",
    "cypress": "5.5.0",
    "dotenv": "8.2.0",
    "dotenv-cli": "4.0.0",
    "eslint": "7.12.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-formatter-pretty": "4.0.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jest-dom": "3.2.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.9.2",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "jest-runner-eslint": "0.10.0",
    "jest-silent-reporter": "0.3.0",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.0",
    "start-server-and-test": "1.11.5",
    "vercel": "20.1.2"
  },
  "resolutions": {
    "**/cypress": "5.5.0"
  }
