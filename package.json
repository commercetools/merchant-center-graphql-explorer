{
  "name": "merchant-center-graphql-explorer",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "postinstall": "manypkg check",
    "prebuild": "yarn compile-data",
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "compile-html": "NODE_ENV=production mc-scripts compile-html --transformer $(pwd)/vercel-deployment/transformer.js",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "extract-intl": "formatjs extract --format=$(pwd)/intl-formatter.js --out-file=$(pwd)/src/i18n/data/core.json 'src/**/!(*.spec).js'",
    "compile-data": "formatjs compile-folder --format=$(pwd)/intl-formatter.js --ast src/i18n/data src/i18n/compiled-data",
    "lint": "jest --config jest.eslint.config.js",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:e2e": "cypress run",
    "versions:appkit": "manypkg upgrade @commercetools-frontend",
    "versions:uikit": "manypkg upgrade @commercetools-uikit"
  },
  "dependencies": {
    "@apollo/client": "3.3.21",
    "@commercetools-frontend/actions-global": "20.8.0",
    "@commercetools-frontend/application-components": "20.8.0",
    "@commercetools-frontend/application-shell": "20.8.0",
    "@commercetools-frontend/assets": "20.8.0",
    "@commercetools-frontend/cypress": "20.8.0",
    "@commercetools-frontend/eslint-config-mc-app": "20.8.0",
    "@commercetools-frontend/i18n": "20.8.0",
    "@commercetools-frontend/jest-preset-mc-app": "20.8.0",
    "@commercetools-frontend/mc-scripts": "20.8.0",
    "@commercetools-frontend/permissions": "20.8.0",
    "@commercetools-uikit/flat-button": "12.2.2",
    "@commercetools-uikit/icons": "12.2.2",
    "@commercetools-uikit/notifications": "12.2.2",
    "@commercetools-uikit/spacings": "12.2.2",
    "@commercetools-uikit/text": "12.2.2",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@formatjs/cli": "4.2.29",
    "@manypkg/cli": "0.18.0",
    "@testing-library/cypress": "8.0.0",
    "@testing-library/react": "12.0.0",
    "cypress": "7.7.0",
    "dotenv": "10.0.0",
    "eslint": "7.31.0",
    "eslint-formatter-pretty": "4.1.0",
    "graphiql": "1.4.2",
    "graphiql-explorer": "0.6.3",
    "graphql": "15.5.1",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-runner-eslint": "0.10.1",
    "jest-silent-reporter": "0.5.0",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.20.6",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "redux": "4.1.0",
    "start-server-and-test": "1.13.1",
    "vercel": "23.0.1"
  }
}
