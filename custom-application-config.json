{
  "name": "GraphQL Explorer",
  "entryPointUriPath": "graphql-explorer-commercetools",
  "cloudIdentifier": "gcp-eu",
  "env": {
    "development": {
      "initialProjectKey": "${env:CTP_INITIAL_PROJECT_KEY}"
    },
    "production": {
      "applicationId": "${env:APPLICATION_ID}",
      "url": "https://merchant-center-graphql-explorer-commercetools-playground.vercel.app"
    }
  },
  "oAuthScopes": {
    "view": [
      "view_api_clients",
      "view_cart_discounts",
      "view_customer_groups",
      "view_customers",
      "view_discount_codes",
      "view_import_containers",
      "view_orders",
      "view_payments",
      "view_products",
      "view_project_settings",
      "view_shipping_methods",
      "view_states",
      "view_stores",
      "view_tax_categories",
      "view_types"
    ],
    "manage": []
  },
  "headers": {
    "csp": {
      "connect-src": [
        "${env:HOST_GCP_STAGING}"
      ]
    }
  },
  "menuLinks": {
    "icon": "${path:@commercetools-frontend/assets/application-icons/rocket.svg}",
    "permissions": ["ViewGraphqlExplorerCommercetools"],
    "defaultLabel": "Graphql Explorer",
    "labelAllLocales": [],
    "submenuLinks": [
      {
        "uriPath": "platform",
        "permissions": ["ViewGraphqlExplorerCommercetools"],
        "defaultLabel": "commercetools platform",
        "labelAllLocales": []
      },
      {
        "uriPath": "users",
        "permissions": ["ViewGraphqlExplorerCommercetools"],
        "defaultLabel": "Users",
        "labelAllLocales": []
      },
      {
        "uriPath": "settings",
        "permissions": ["ViewGraphqlExplorerCommercetools"],
        "defaultLabel": "Settings",
        "labelAllLocales": []
      }
    ]
  }
}