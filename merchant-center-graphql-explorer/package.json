    "@commercetools-frontend/jest-preset-mc-app": "22.17.2",
    "@commercetools-frontend/mc-scripts": "22.17.2",
    "@commercetools-frontend/permissions": "22.17.2",
    "@commercetools-uikit/constraints": "18.1.0",
